<mxEditor>
	<include name="./assets/mxgraph/config/keyhandler-commons.xml"/>
  <ui>
    <add as="graph" element="graph"/>
    <add as="toolbar" element="toolbar"/>
  </ui>
  <Array as="templates">
    <add as="connector">
      <Connector label="" type="">
        <mxCell edge="1">
          <mxGeometry as="geometry" relative="1"/>
        </mxCell>
      </Connector>
    </add>
    <add as="FileOrder">
      <FileOrder label="fileOrder" agent="" regex=".*" directory="" checkSteadyState="true">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="120" height="50"/>
        </mxCell>
      </FileOrder>
    </add>
    <add as="Job">
      <Job name="/Job" title="" agent="">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="200" height="50"/>
        </mxCell>
      </Job>
    </add>
    <add as="If">
      <If label="if" predicate="returnCode > 0">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="150" height="80"/>
        </mxCell>
      </If>
    </add>
    <add as="Retry">
      <Retry label="retry" repeat="10" delay="0">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="150" height="80"/>
        </mxCell>
      </Retry>
    </add>
    <add as="Terminate">
      <Terminate label="terminate" message="order prematurely completed">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </Terminate>
    </add>
    <add as="Abort">
      <Abort label="abort" message="order failed">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </Abort>
    </add>
    <add as="Fork">
      <Fork label="fork">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="90" height="90"/>
        </mxCell>
      </Fork>
    </add>
    <add as="Await">
      <Await label="await">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="90" height="90"/>
        </mxCell>
      </Await>
    </add>
    <add as="Try">
      <Try label="try">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="94" height="94"/>
        </mxCell>
      </Try>
    </add>
    <add as="Catch">
      <Catch label="catch">
        <mxCell vertex="1">
          <mxGeometry as="geometry" width="100" height="40"/>
        </mxCell>
      </Catch>
    </add>
    <add as="symbol">
      <Symbol label="Instruction" description="">
        <mxCell vertex="1" style="symbol;image=images/symbols/event.png">
          <mxGeometry as="geometry" width="75" height="75"/>
        </mxCell>
      </Symbol>
    </add>
  </Array>
</mxEditor>

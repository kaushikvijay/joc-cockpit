<mxEditor defaultGroup="group" defaultEdge="connector">
  <include name="./assets/mxgraph/config/editor-commons.xml"/>
  <mxGraph as="graph" alternateEdgeStyle="verticalConnector" allowLoops="1" dropEnabled="1">
    <add as="isAutoSizeCell"><![CDATA[
			function(cell)
			{
				return mxUtils.isNode(this.model.getValue(cell), 'text');
			}
		]]></add>
    <add as="isSwimlane"><![CDATA[
			function (cell)
			{
				return mxUtils.isNode(this.model.getValue(cell), 'container');
			}
		]]></add>
    <add as="getTooltipForCell"><![CDATA[
			function(cell)
			{
				var label = cell.getAttribute('label');
				var style = cell.getStyle();

				return ((label != null) ? ('<b>' + label +
						'</b> : '') +
						'Connections: ' + cell.getEdgeCount()+
						'<br>Children: ' + cell.getChildCount();
			}
		]]></add>
    <add as="convertValueToString"><![CDATA[
			function(cell)
			{
				return cell.getAttribute('label');
			}
		]]></add>
    <mxStylesheet as="stylesheet">
      <add as="text">
        <add as="shape" value="rectangle"/>
        <add as="perimeter" value="rectanglePerimeter"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="top"/>
        <add as="shadow" value="0"/>
        <add as="strokeColor" value="none"/>
        <add as="fillColor" value="transparent"/>
        <add as="gradientColor" value="none"/>
      </add>
      <add as="defaultVertex" extend="text">
        <add as="shape" value="rectangle"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="middle"/>
        <add as="strokeColor" value="#fafafa"/>
        <add as="fontColor" value="#fafafa"/>
      </add>
      <add as="group">
        <add as="shape" value="rectangle"/>
        <add as="perimeter" value="rectanglePerimeter"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="middle"/>
        <add as="strokeColor" value="#fafafa"/>
        <add as="dashed" value="1"/>
      </add>
      <add as="defaultEdge">
        <add as="shape" value="connector"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="center"/>
        <add as="rounded" value="1"/>
        <add as="labelBackgroundColor" value="#333"/>
        <add as="strokeColor" value="#fafafa"/>
        <add as="strokeWidth" value="1"/>
        <add as="edgeStyle" value="elbowEdgeStyle"/>
        <add as="endArrow" value="classic"/>
      </add>
      <add as="verticalConnector">
        <add as="elbow" value="vertical"/>
      </add>
      <add as="straightConnector">
        <add as="shape" value="connector"/>
        <add as="endArrow" value="classic"/>
        <add as="edgeStyle">null</add>
      </add>
      <add as="arrowConnector">
        <add as="shape" value="arrow"/>
        <add as="fillColor" value="#fafafa"/>
        <add as="endSize" value="20"/>
        <remove as="edgeStyle"/>
      </add>
      <add as="swimlane">
        <add as="shape" value="swimlane"/>
        <add as="shadow" value="0"/>
        <add as="startSize" value="23"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="top"/>
      </add>
      <add as="job">
        <add as="rounded" value="1"/>
        <add as="shape" value="label"/>
        <add as="image" value="./assets/mxgraph/images/symbols/job-white.png"/>
        <add as="imageWidth" value="20"/>
        <add as="imageHeight" value="20"/>
         <add as="strokeColor" value="#C3D9FF"/>
				<add as="fillColor" value="#C3D9FF"/>
        <add as="gradientColor" value="#333"/>
      </add>
      <add as="rectangle" extend="rhombus">
				<add as="shape" value="rectangle"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="align" value="center"/>
			</add>
      <add as="dashRectangle" extend="rhombus">
				<add as="shape" value="rectangle"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="align" value="center"/>
				<add as="dashed" value="1"/>
				<add as="shadow" value="0"/>
				<add as="opacity" value="70"/>
         <add as="strokeColor" value="#FFCF8c"/>
				<add as="fillColor" value="#FFCF8c"/>
        <add as="gradientColor" value="#333"/>
			</add>
      <add as="catch" extend="rhombus">
				<add as="shape" value="rectangle"/>
				<add as="perimeter" value="rectanglePerimeter"/>
				<add as="align" value="center"/>
        <add as="strokeColor" value="#FFCF8c"/>
				<add as="fillColor" value="#FFCF8c"/>
        <add as="gradientColor" value="#333"/>
			</add>
      <add as="ellipse">
        <add as="shape" value="ellipse"/>
        <add as="perimeter" value="ellipsePerimeter"/>
        <add as="fillColor" value="transparent"/>
      </add>
        <add as="if">
        <add as="shape" value="rhombus"/>
        <add as="perimeter" value="rhombusPerimeter"/>
        <add as="strokeColor" value="#CDEB8B"/>
				<add as="fillColor" value="#CDEB8B"/>
        <add as="rounded" value="1"/>
        <add as="gradientColor" value="#333"/>
      </add>
      <add as="retry">
        <add as="shape" value="rhombus"/>
        <add as="perimeter" value="rhombusPerimeter"/>
	      <add as="strokeColor" value="#FFC7C7"/>
				<add as="fillColor" value="#FFC7C7"/>
        <add as="rounded" value="1"/>
        <add as="gradientColor" value="#333"/>
      </add>
      <add as="try">
        <add as="shape" value="rhombus"/>
        <add as="perimeter" value="rhombusPerimeter"/>
        <add as="strokeColor" value="#FFCF8A"/>
				<add as="fillColor" value="#FFCF8A"/>
        <add as="rounded" value="1"/>
        <add as="gradientColor" value="#333"/>
      </add>
      <add as="fork">
        <add as="shape" value="rhombus"/>
        <add as="perimeter" value="rhombusPerimeter"/>
        <add as="strokeColor" value="#8fb0f2"/>
				<add as="fillColor" value="#8fb0f2"/>
        <add as="rounded" value="1"/>
        <add as="gradientColor" value="#333"/>
      </add>
      <add as="symbol">
        <add as="shape" value="image"/>
        <add as="perimeter" value="rectanglePerimeter"/>
        <add as="fontSize" value="12"/>
        <add as="align" value="center"/>
        <add as="verticalAlign" value="top"/>
        <add as="verticalLabelPosition" value="bottom"/>
        <add as="fontColor" value="#fafafa"/>
      </add>
    </mxStylesheet>
    <mxGraphModel as="model">
      <root>
        <mxCell id="0"/>
        <mxCell parent="0"/>
      </root>
    </mxGraphModel>
  </mxGraph>
  <mxDefaultToolbar as="toolbar">
    <add as="connect" mode="connect" icon="./assets/mxgraph/images/symbols/connect.gif"/>
    <add as="show" action="show" icon="./assets/mxgraph/images/preview-white.png"/>
    <hr/>
    <add as="Abort" template="Abort" style="symbol;image=./assets/mxgraph/images/symbols/abort-white.png"
         icon="./assets/mxgraph/images/symbols/abort-white.png"/>
    <add as="Job" template="job" icon="./assets/mxgraph/images/symbols/job-white.png"/>
    <add as="FileOrder" template="rectangle" icon="./assets/mxgraph/images/symbols/order-white.png"/>
    <add as="Terminate" template="Terminate" style="symbol;image=./assets/mxgraph/images/symbols/terminate-white.png"
         icon="./assets/mxgraph/images/symbols/terminate-white.png"/>
    <hr/>
    <add as="Await" template="Await" style="symbol;image=./assets/mxgraph/images/symbols/timer-white.png"
         icon="./assets/mxgraph/images/symbols/timer-white.png"/>
    <add as="Fork" template="Fork" style="symbol;image=./assets/mxgraph/images/symbols/fork-white.png"
         icon="./assets/mxgraph/images/symbols/fork-white.png"/>
    <add as="If" template="If" style="if" icon="./assets/mxgraph/images/symbols/inclusive-white.png"/>
    <add as="Retry" template="Retry" style="retry" icon="./assets/mxgraph/images/symbols/retry-white.png"/>
     <add as="Try" template="Try" style="try" icon="./assets/mxgraph/images/symbols/try-white.png"/>
  </mxDefaultToolbar>
</mxEditor>
